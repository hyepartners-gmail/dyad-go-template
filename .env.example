# Runtime environment
GO_ENV=production
PORT=8080

# Google Cloud settings
PROJECT_ID=my-gcloud-project-id
REGION=us-central1

# GCP service account credentials path (used locally only)
GOOGLE_APPLICATION_CREDENTIALS=./secrets/service-account.json

# JWT secret (override in Secret Manager for prod)
JWT_SECRET=change-this-to-a-secret-value
